{
  "cli": {
    "version": ">= 3.5.2",
    "requireCommit": false
  },
  "build": {
    "base": {
      "node": "20.11.1",
      "env": {
        "EXPO_APP_ENV": "production"
      },
     
      "android": {
        "image": "latest",
        "env": {
          "PLATFORM": "android"
        }
      },
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "channel": "development",
      "distribution": "internal",
      "env": {
        "EXPO_APP_ENV": "dev"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug"
      }
       
    },
    "staging": {
      "extends": "base",
      "channel": "staging",
      "distribution": "internal",
      "env": {
        "EXPO_APP_ENV": "stg"
      }
       
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "env": {
        "EXPO_APP_ENV": "prod"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
