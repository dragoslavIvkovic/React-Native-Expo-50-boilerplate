{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "jsx": "react-native",
    "paths": {
      "@assets/*": ["./assets/*"],
      "@components/*": ["./src/components/*"],
      "@screens/*": ["./src/screens/*"],
      "@navigator": ["./src/navigator/index.ts"],
      "@navigator/*": ["./src/navigator/*"],
      "@utils/*": ["./src/utils/*"],
      "@theme": ["./src/theme/index.ts"],
      "@modules/*": ["./src/modules/*"],
       "@store/*": ["./src/store/*"],
    }
  }
}
